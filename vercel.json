{
	"builds":[{
		"src": "khwebsite/wsgi.py",
		"use": "@vercel/python",
		"config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
	}],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "khwebsite/wsgi.py",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
		}
	]
}